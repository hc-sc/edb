<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.oecd.org/GHSTS" targetNamespace="http://www.oecd.org/GHSTS" elementFormDefault="qualified" attributeFormDefault="unqualified" version="01.00.00">
	<xs:include schemaLocation="ghsts-picklists.xsd"/>
	<xs:element name="GHSTS">
		<xs:annotation>
			<xs:documentation>Root level structure </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="RECEIVERS" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>List of RAs (Regulatory Authorities for whom the dossier is being submitted)</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="RECEIVER" minOccurs="1" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Definition block for a Regulatory Authority</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="METADATA_STATUS" type="TYPE_METADATA_STATUS_STRUCT" minOccurs="1" maxOccurs="1"/>
										<xs:element name="SHORT_NAME" type="string20" minOccurs="1" maxOccurs="1">
											<xs:annotation>
												<xs:documentation>Typical abbreviation eg. EPA, BVL</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="ROLE" type="string255" minOccurs="0" maxOccurs="1">
											<xs:annotation>
												<xs:documentation>Role of the RA</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="SENDER" minOccurs="1" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>RA specific company submitting the dossier</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="COMPANY_CONTACT_REGULATORY_ROLE" type="string255" minOccurs="0" maxOccurs="1">
														<xs:annotation>
															<xs:documentation>The Regulatory Role of the contact role</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="REMARK" type="string2000" minOccurs="0" maxOccurs="1">
														<xs:annotation>
															<xs:documentation>Area to communicate additional company contact details.</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
												<xs:attribute name="To_Legal_Entity_Id" type="xs:IDREF" use="required"/>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="Id" type="xs:ID" use="required"/>
									<xs:attribute name="To_Legal_Entity_Id" type="xs:IDREF" use="required"/>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="PRODUCT" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Metadata of the product</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="METADATA_STATUS" type="TYPE_METADATA_STATUS_STRUCT" minOccurs="1" maxOccurs="1"/>
							<xs:element name="PRODUCT_PID" type="PIDType" minOccurs="1" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>World wide unique identifier of the product. This element is not for the user interface.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="GENERIC_PRODUCT_NAME" type="string255" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>The generic name (common name or active ingredient): ibuprofen</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="PRODUCT_RA" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>The RA specific fields </xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="PRODUCT_NAME" type="string2000" minOccurs="0" maxOccurs="1">
											<xs:annotation>
												<xs:documentation>Contains the product name associated with the dossier.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="ADMIN_NUMBER" minOccurs="0" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="ADMIN_NUMBER_TYPE" minOccurs="1" maxOccurs="1">
														<xs:annotation>
															<xs:documentation>Represents the type of the Admin Number. </xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="VALUE" type="EXTENSION_TYPE_ADMIN_NUMBER_TYPE">
																	<xs:annotation>
																		<xs:documentation>Code according restricted picklist TYPE_ADMIN_NUMBER_TYPE; other values are submitted via attribute 'Other_Value'</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="VALUE_DECODE" type="string255">
																	<xs:annotation>
																		<xs:documentation>Long text of 'Value' or 'Other_Value'</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="IDENTIFIER" type="string255" minOccurs="1" maxOccurs="1">
														<xs:annotation>
															<xs:documentation>The RA specific identifier of the product.</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="To_Specific_for_RA_Id" type="xs:IDREF" use="required"/>
								</xs:complexType>
							</xs:element>
							<xs:element name="FORMULATION_TYPE" minOccurs="0" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>A 2 character code describing the formulation of the product following GIFAP rules. </xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="VALUE" type="EXTENSION_TYPE_FORMULATION_TYPE">
											<xs:annotation>
												<xs:documentation>Code according restricted picklist TYPE_FORMULATION_TYPE; other values are submitted via attribute 'Other_Value'</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="VALUE_DECODE" type="string255">
											<xs:annotation>
												<xs:documentation>Long text of 'Value' or 'Other_Value'</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="INGREDIENTS" minOccurs="1" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>List of Ingredients</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="INGREDIENT" minOccurs="1" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Metadata for each Component</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="QUANTITY" type="xs:decimal" minOccurs="0" maxOccurs="1">
														<xs:annotation>
															<xs:documentation/>
														</xs:annotation>
													</xs:element>
													<xs:element name="UNIT" minOccurs="0" maxOccurs="1">
														<xs:annotation>
															<xs:documentation>Unit of Quantitiy</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="VALUE" type="EXTENSION_TYPE_UNIT">
																	<xs:annotation>
																		<xs:documentation>Code according restricted picklist TYPE_UNIT; other values are submitted via attribute 'Other_Value'</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="VALUE_DECODE" type="string255">
																	<xs:annotation>
																		<xs:documentation>Long text of 'Value' or 'Other_Value'</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
												<xs:attribute name="To_Substance_Id" type="xs:IDREF" use="required"/>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="DOSSIER" minOccurs="1" maxOccurs="1">
								<xs:annotation>
									<xs:documentation>Metadata for the Dossier-Life Cycle Management</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="DOSSIER_PID" type="PIDType" minOccurs="1" maxOccurs="1">
											<xs:annotation>
												<xs:documentation>World wide unique identifier for Dossiers. </xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="DOSSIER_DESCRIPTION_TITLE" type="string2000" minOccurs="1" maxOccurs="1">
											<xs:annotation>
												<xs:documentation>The description of the Dossier self designated by the registrant.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="DOSSIER_COMP_ID" type="string20">
											<xs:annotation>
												<xs:documentation>Short code for the dossier, starting with a 3-letter company code</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="REFERENCED_DOSSIER" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Metadata of the referenced Dossier</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="REFERENCED_DOSSIER_NUMBER" type="string255" maxOccurs="1">
														<xs:annotation>
															<xs:documentation>The reference number (or PID) to the old dossier.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="REFERENCED_DOSSIER_REASON" type="string2000" maxOccurs="1">
														<xs:annotation>
															<xs:documentation>Reason for the reference</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="DOSSIER_RA" minOccurs="1" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>The RA specific fields </xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="REGULATORY_TYPE" minOccurs="1" maxOccurs="1">
														<xs:annotation>
															<xs:documentation>Legislation, act, regulation or directive under which the submission is made</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="VALUE" type="EXTENSION_TYPE_REGULATORY_TYPE">
																	<xs:annotation>
																		<xs:documentation>Code according restricted picklist TYPE_REGULATORY_TYPE; other values are submitted via attribute 'Other_Value'</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="VALUE_DECODE" type="string255">
																	<xs:annotation>
																		<xs:documentation>Long text of 'Value' or 'Other_Value'</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="APPLICATION_TYPE" minOccurs="1" maxOccurs="1">
														<xs:annotation>
															<xs:documentation>Specifies the type of application contained in the dossier. </xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="VALUE" type="EXTENSION_TYPE_APPLICATION_TYPE">
																	<xs:annotation>
																		<xs:documentation>Code according restricted picklist TYPE_APPLICATION_TYPE; other values are submitted via attribute 'Other_Value'</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="VALUE_DECODE" type="string255">
																	<xs:annotation>
																		<xs:documentation>Long text of 'Value' or 'Other_Value'</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="PROJECT_ID_NUMBER" type="string255" minOccurs="0" maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation>The project identifier(s) assigned by the RA relevant to the dossier.</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
												<xs:attribute name="To_Specific_for_RA_Id" type="xs:IDREF" use="required"/>
											</xs:complexType>
										</xs:element>
										<xs:element name="SUBMISSION" minOccurs="1" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Metadata of Submission</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="SUBMISSION_NUMBER" type="submissionnumberType" minOccurs="1" maxOccurs="1">
														<xs:annotation>
															<xs:documentation>Identifies the sequence number of the submission inside a dossier.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="SUBMISSION_VERSION_DATE" type="xs:date" minOccurs="1" maxOccurs="1">
														<xs:annotation>
															<xs:documentation>Identifies the date of the submission as defined by the registrant.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="SUBMISSION_TITLE" type="string2000" minOccurs="1" maxOccurs="1">
														<xs:annotation>
															<xs:documentation>Title of the submission.</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="INCREMENTAL" type="xs:boolean" minOccurs="1" maxOccurs="1">
														<xs:annotation>
															<xs:documentation>Flag of the submission type (full or incremental)</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="DOCUMENTS" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>List of Documents</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="DOCUMENT" minOccurs="1" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Metadata of a Document</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="DOCUMENT_RA" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>RA specific Metadata</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="METADATA_STATUS" type="TYPE_METADATA_STATUS_STRUCT"/>
													<xs:element name="DATA_PROTECTION">
														<xs:annotation>
															<xs:documentation>Indicates a data protection claim for required or not required documentation. </xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="VALUE" type="TYPE_DATA_PROTECTION">
																	<xs:annotation>
																		<xs:documentation>Code according restricted picklist TYPE_DATA_PROTECTION; NO other values are allowed</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="VALUE_DECODE" type="string255">
																	<xs:annotation>
																		<xs:documentation>Long text of 'Value' or 'Other_Value'</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="DATA_REQUIREMENT">
														<xs:annotation>
															<xs:documentation/>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="VALUE" type="TYPE_DATA_REQUIREMENT">
																	<xs:annotation>
																		<xs:documentation>Code according restricted picklist TYPE_DATA_REQUIREMENT; NO other values are allowed</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="VALUE_DECODE" type="string255">
																	<xs:annotation>
																		<xs:documentation>Long text of 'Value' or 'Other_Value'</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="DOCUMENT_COMMENT" type="string2000" minOccurs="0" maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation>130) Allows the submitting company to communicate remarks to the Regulatory Authority at the document level. </xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="OTHER_NATIONAL_GUIDELINE" minOccurs="0" maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation>Additional references to other national guidelines. This is a multiple block to make references from one document into one national guideline to different annex-points. </xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="GUIDELINE_SYSTEM" type="string20">
																	<xs:annotation>
																		<xs:documentation>Other guideline system eg. DACO, OPPTS</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="GUIDELINE_NUMBER" type="string20">
																	<xs:annotation>
																		<xs:documentation>Number in the other guideline system</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="RA_DOCUMENT_NUMBER" minOccurs="0">
														<xs:annotation>
															<xs:documentation>The document identification number assigned by Regulatory Authority eg. MRID</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="RA_DOCUMENT_NUMBER_TYPE">
																	<xs:annotation>
																		<xs:documentation/>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="VALUE" type="EXTENSION_TYPE_RA_DOCUMENT_NUMBER_TYPE">
																				<xs:annotation>
																					<xs:documentation>Code according restricted picklist TYPE_RA_DOCUMENT_NUMBER_TYPE; other values are submitted via attribute 'Other_Value'</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="VALUE_DECODE" type="string255">
																				<xs:annotation>
																					<xs:documentation>Long text of 'Value' or 'Other_Value'</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
																<xs:element name="IDENTIFIER" type="string255">
																	<xs:annotation>
																		<xs:documentation>Identifier string</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="ALREADY_SUBMITTED" type="xs:boolean"/>
																<xs:element name="SUBMISSION_CONTEXT" minOccurs="0" maxOccurs="unbounded">
																	<xs:annotation>
																		<xs:documentation>Metadata of the referenced Dossier</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:choice>
																			<xs:element name="DOSSIER_PID" type="PIDType">
																				<xs:annotation>
																					<xs:documentation>World wide unique identifier for Dossiers. </xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="DOSSIER_NUMBER" type="string255">
																				<xs:annotation>
																					<xs:documentation>The reference number (or PID) to the old dossier.</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																		</xs:choice>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
												<xs:attribute name="To_Specific_for_RA_Id" type="xs:IDREF" use="required"/>
											</xs:complexType>
										</xs:element>
										<xs:element name="DOCUMENT_GENERIC" minOccurs="1" maxOccurs="1">
											<xs:annotation>
												<xs:documentation>Bibliographic Data and other properties of a Document</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="METADATA_STATUS" type="TYPE_METADATA_STATUS_STRUCT" minOccurs="1" maxOccurs="1"/>
													<xs:element name="DOCUMENT_PID" type="PIDType">
														<xs:annotation>
															<xs:documentation>A worldwide unique document number </xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="DOCUMENT_FAMILY_PID" type="PIDType">
														<xs:annotation>
															<xs:documentation>A machine readable filter to select documents which belongs to one "document family"</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="DOCUMENT_FAMILY" type="string255">
														<xs:annotation>
															<xs:documentation>A simple filter for documents which belongs to one "document family"</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="CONTENT_STATUS_HISTORY">
														<xs:annotation>
															<xs:documentation>Use this element for listing the document history</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence maxOccurs="unbounded">
																<xs:element name="CONTENT_STATUS" type="TYPE_CONTENT_STATUS_STRUCT"/>
																<xs:element name="SUBMISSION_NUMBER" type="submissionnumberType">
																	<xs:annotation>
																		<xs:documentation>Submission number for the given Status type</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="REFERENCED_DOCUMENT" minOccurs="0" maxOccurs="unbounded">
														<xs:annotation>
															<xs:documentation>A concrete reference to document(s) in support of this document with a Document PID</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="REFERENCE_TYPE">
																	<xs:annotation>
																		<xs:documentation>Describes the referenced document(s) relation to the dossier document (picklist)</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="VALUE" type="TYPE_REFERENCE_TYPE">
																				<xs:annotation>
																					<xs:documentation>Code according restricted picklist TYPE_REFERENCE_TYPE; NO other values are allowed</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																			<xs:element name="VALUE_DECODE" type="string255">
																				<xs:annotation>
																					<xs:documentation>Long text of 'Value' or 'Other_Value'</xs:documentation>
																				</xs:annotation>
																			</xs:element>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
																<xs:element name="INTERNAL" type="xs:boolean"/>
																<xs:choice>
