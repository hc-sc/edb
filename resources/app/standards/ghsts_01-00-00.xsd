														<xs:annotation>
															<xs:documentation>The surname of the person</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element name="PHONE" type="string255" minOccurs="0" maxOccurs="1">
														<xs:annotation>
															<xs:documentation/>
														</xs:annotation>
													</xs:element>
													<xs:element name="MOBILE" type="string255" minOccurs="0" maxOccurs="1">
														<xs:annotation>
															<xs:documentation/>
														</xs:annotation>
													</xs:element>
													<xs:element name="FAX" type="string255" minOccurs="0" maxOccurs="1">
														<xs:annotation>
															<xs:documentation/>
														</xs:annotation>
													</xs:element>
													<xs:element name="EMAIL" type="string255" minOccurs="0" maxOccurs="1">
														<xs:annotation>
															<xs:documentation/>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="Id" type="xs:ID" use="required"/>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="SUBSTANCES" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>List of Substances (Ingredients and other referenced substances like metabolites)</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="SUBSTANCE" minOccurs="1" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Metadata for each referenced substance</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="METADATA_STATUS" type="TYPE_METADATA_STATUS_STRUCT" minOccurs="1" maxOccurs="1"/>
										<xs:element name="SUBSTANCE_NAME" type="string2000" minOccurs="1" maxOccurs="1">
											<xs:annotation>
												<xs:documentation>Common name if available otherwise internal name</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="SUBSTANCE_PID" type="PIDType" minOccurs="0" maxOccurs="1">
											<xs:annotation>
												<xs:documentation>PID of the pure reference substance. This element is not for the user interface.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="SUBSTANCE_IDENTIFIER" minOccurs="0" maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>Eg. CASno, ECNO for pure ingredients</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="SUBSTANCE_IDENTIFIER_TYPE" minOccurs="1" maxOccurs="1">
														<xs:annotation>
															<xs:documentation/>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence>
																<xs:element name="VALUE" type="EXTENSION_TYPE_SUBSTANCE_IDENTIFIER_TYPE">
																	<xs:annotation>
																		<xs:documentation>Code according restricted picklist TYPE_SUBSTANCE_IDENTIFIER_TYPE; other values are submitted via attribute 'Other_Value'</xs:documentation>
																	</xs:annotation>
																</xs:element>
																<xs:element name="VALUE_DECODE" type="string255">
																	<xs:annotation>
																		<xs:documentation>Long text of 'Value' or 'Other_Value'</xs:documentation>
																	</xs:annotation>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="IDENTIFIER" type="string255" minOccurs="1" maxOccurs="1">
														<xs:annotation>
															<xs:documentation>Identifier string</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="Id" type="xs:ID" use="required"/>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="USED_TEMPLATES" minOccurs="0" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Placeholder for a reference to external XML with overview of all stylesheets required for rendering of embedded XML files. This XML follows a different XSD</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="specificationversion" type="versionnumberType" use="required"/>
		</xs:complexType>
		<!-- Validation of PIDs in submission package -->
		<xs:key name="KeyFilePID">
			<xs:selector xpath="./FILES/FILE/FILE_GENERIC"/>
			<xs:field xpath="FILE_PID"/>
		</xs:key>
		<xs:key name="KeyDocumentPID">
			<xs:selector xpath="./DOCUMENTS/DOCUMENT/DOCUMENT_GENERIC"/>
			<xs:field xpath="DOCUMENT_PID"/>
		</xs:key>
		<!-- Validation of unique FILENAME definition in submission package -->
		<xs:key name="KeyFileName">
			<xs:selector xpath="./FILES/FILE/FILE_GENERIC"/>
			<xs:field xpath="FILENAME"/>
		</xs:key>
		<!-- Validation of unique string IDs stored in attributes in submission package -->
		<xs:key name="KeyDocumentId">
			<xs:selector xpath="./DOCUMENTS/DOCUMENT"/>
			<xs:field xpath="@Id"/>
		</xs:key>
		<xs:key name="KeyFileId">
			<xs:selector xpath="./FILES/FILE"/>
			<xs:field xpath="@Id"/>
		</xs:key>
		<xs:key name="KeyLegalEntityId">
			<xs:selector xpath="./LEGAL_ENTITIES/LEGAL_ENTITY"/>
			<xs:field xpath="@Id"/>
		</xs:key>
		<xs:key name="KeyReceiverId">
			<xs:selector xpath="./RECEIVERS/RECEIVER"/>
			<xs:field xpath="@Id"/>
		</xs:key>
		<xs:key name="KeySubstanceId">
			<xs:selector xpath="./SUBSTANCES/SUBSTANCE"/>
			<xs:field xpath="@Id"/>
		</xs:key>
		<!-- Validation of referential integrity in submission package -->
		<xs:keyref name="RefIntegrityIngredientToSubstance" refer="KeySubstanceId">
			<xs:selector xpath="./PRODUCT/INGREDIENTS/INGREDIENT"/>
			<xs:field xpath="@To_Substance_Id"/>
		</xs:keyref>
		<xs:keyref name="RefIntegritySenderToLegalentity" refer="KeyLegalEntityId">
			<xs:selector xpath="./RECEIVERS/RECEIVER/SENDER"/>
			<xs:field xpath="@To_Legal_Entity_Id"/>
		</xs:keyref>
		<xs:keyref name="RefIntegrityReceiverToLegalentity" refer="KeyLegalEntityId">
			<xs:selector xpath="./RECEIVERS/RECEIVER"/>
			<xs:field xpath="@To_Legal_Entity_Id"/>
		</xs:keyref>
		<xs:keyref name="RefIntegrityProduct_RAToReceiver" refer="KeyReceiverId">
			<xs:selector xpath="./PRODUCT/PRODUCT_RA"/>
			<xs:field xpath="@To_Specific_for_RA_Id"/>
		</xs:keyref>
		<xs:keyref name="RefIntegrityDossier_RAToReceiver" refer="KeyReceiverId">
			<xs:selector xpath="./PRODUCT/DOSSIER/DOSSIER_RA"/>
			<xs:field xpath="@To_Specific_for_RA_Id"/>
		</xs:keyref>
		<xs:keyref name="RefIntegrityDocument_RAToReceiver" refer="KeyReceiverId">
			<xs:selector xpath="./DOCUMENTS/DOCUMENT/DOCUMENT_RA"/>
			<xs:field xpath="@To_Specific_for_RA_Id"/>
		</xs:keyref>
		<xs:keyref name="RefIntegrityFile_RAToReceiver" refer="KeyReceiverId">
			<xs:selector xpath="./FILES/FILE/FILE_RA"/>
			<xs:field xpath="@To_Specific_for_RA_Id"/>
		</xs:keyref>
		<xs:keyref name="RefIntegrityDocumentToFile" refer="KeyFileId">
			<xs:selector xpath="./DOCUMENTS/DOCUMENT/DOCUMENT_GENERIC/REFERENCED_TO_FILE"/>
			<xs:field xpath="@To_File_Id"/>
		</xs:keyref>
		<xs:keyref name="RefIntegrityDocumentToSubstance" refer="KeySubstanceId">
			<xs:selector xpath="./DOCUMENTS/DOCUMENT/DOCUMENT_GENERIC/RELATED_TO_SUBSTANCE"/>
			<xs:field xpath="@To_Substance_Id"/>
		</xs:keyref>
		<xs:keyref name="RefIntegrityTocnodeToDocument" refer="KeyDocumentId">
			<xs:selector xpath=".//TOC_NODE/DOCUMENT_REFERENCE"/>
			<xs:field xpath="@To_Document_Id"/>
		</xs:keyref>
	</xs:element>
	<xs:complexType name="TYPE_TOC_NODE">
		<xs:annotation>
			<xs:documentation>A ToC has a lot of TOC-Nodes, identified by unique TOC NODE NAME and PID</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="NODE_NAME" type="string20" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Unique String inside of all TOCS, usable for cross-references from document to the TOC_NODE_NAME</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NODE_HEADING" type="string255">
				<xs:annotation>
					<xs:documentation>Heading text </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LOGICAL_DELETED" type="xs:boolean"/>
			<xs:element name="TOC_NODE_PID" type="PIDType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>World wide unique identifier for TOC Nodes. This PID should be the basis for cross references between different TOC's </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EMPTY_NODE" type="xs:boolean" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Are there any documents submitted in this ToC Node incl. Sub-Nodes?</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TOC2DOC" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:attribute name="To_Document_Id" use="required"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="TOC_NODE" type="TYPE_TOC_NODE" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TYPE_METADATA_STATUS_STRUCT">
		<xs:annotation>
			<xs:documentation>Type for compund metadata status element, used in various elements</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="VALUE" type="TYPE_METADATA_STATUS">
				<xs:annotation>
					<xs:documentation>Code according restricted picklist TYPE_METADATA_STATUS; NO other values are allowed</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="VALUE_DECODE" type="string20">
				<xs:annotation>
					<xs:documentation>Long text of 'Value' or 'Other_Value'</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TYPE_CONTENT_STATUS_STRUCT">
		<xs:annotation>
			<xs:documentation>Type for compound content status element, used in FILE and DOCUMENT</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="VALUE" type="TYPE_CONTENT_STATUS">
				<xs:annotation>
					<xs:documentation>Code according restricted picklist TYPE_CONTENT_STATUS; NO other values are allowed</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="VALUE_DECODE" type="string20">
				<xs:annotation>
					<xs:documentation>Long text of 'Value' or 'Other_Value'</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="md5Type">
		<xs:annotation>
			<xs:documentation>MD5 checksum definition, used in FILE</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[a-f,A-F,0-9]{32}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="string20">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="string255">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="255"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="string2000">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="2000"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="filenamestring">
		<xs:annotation>
			<xs:documentation>definition of relative paths for filenames, used in FILE</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:whiteSpace value="preserve"/>
			<xs:minLength value="1"/>
			<xs:maxLength value="230"/>
			<xs:pattern value="(\.\./)\d{2}/(([a-zA-Z0-9_\-\.]+)/)*([a-zA-Z0-9_\-\.\(\)]+)"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PIDType">
		<xs:annotation>
			<xs:documentation>definition of Persistent Identifier (PID) according to URN pattern as defined in RFC 2141</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:anyURI">
			<xs:pattern value="urn:[a-zA-Z0-9][a-zA-Z0-9-]{1,31}:([a-zA-Z0-9()+,.:=@;$_!*'-]|%[0-9A-Fa-f]{2})+"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="uuidType">
		<xs:annotation>
			<xs:documentation>definition of UUID according to canonical representation</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[a-f0-9]{8}\-[a-f0-9]{4}\-[a-f0-9]{4}\-[a-f0-9]{4}\-[a-f0-9]{12}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="submissionnumberType">
		<xs:annotation>
			<xs:documentation>definition of submission number as two digit number, e.g. 01,02,03 ... to be consistent with name of folder in submission package</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{2}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="versionnumberType">
		<xs:annotation>
			<xs:documentation>Version number: six digits, e.g 01.00.00</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{2}\.\d{2}\.\d{2}"/>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>
