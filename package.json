      "core-js": "npm:core-js@^1.1.4"
    }
  },
  "devDependencies": {
    "babel-loader": "^6.2.4",
    "css-loader": "^0.23.1",
    "del": "^2.2.1",
    "electron-builder": "^5.23.2",
    "electron-connect": "^0.4.2",
    "electron-prebuilt": "^1.2.3",
    "eslint": "^3.3.0",
    "file-loader": "^0.9.0",
    "gulp-rename": "^1.2.2",
    "q": "^1.4.1",
