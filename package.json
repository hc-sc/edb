{
  "name": "electronic_dossier_builder",
  "version": "1.3.0",
  "description": "eDossierBuilder",
  "main": "src/index.js",
  "scripts": {
    "postinstall": "node copyjsonixschema.js && npm i src/tingodb-0.5.0.tgz && npm i src/tungus-0.0.5.tgz",
    "bundle": "webpack --watch",
    "start": "electron build",
    "dist": "webpack && build -p never",
    "run": "electron dist",
    "publish": "build",
    "schema": "java -jar ./node_modules/jsonix/lib/jsonix-schema-compiler-full.jar -generateJsonSchema -d ./resources/app/standards/01_00_00 -p GHSTS ./resources/app/standards/ghsts_01-00-00.xsd"
  },
  "author": "Health Canada - Sant√© Canada",
  "contributers": [
    "Aaron Carswell <aaron.carswell@canada.ca>",
    "Lori Currie <lori.currie@canada.ca>",
    "Alex Gagnon <alex.gagnon2@canada.ca>",
    "James Johnson <james.johnson2@canada.ca>",
    "Joseph Mikhael <joseph.mikhael@canada.ca>",
    "Jun Tang <xiang.tang@canada.ca>",
    "Hai Tu <hai.tu@canada.ca>",
    "Daniel Wen <daniel.wen@canada.ca>"
  ],
  "dependencies": {
    "angular": "1.5.9",
    "angular-animate": "1.5.9",
    "angular-aria": "1.5.9",
    "angular-material": "^1.1.1",
    "angular-material-data-table": "^0.10.9",
    "angular-messages": "1.5.9",
    "angular-ui-layout": "^1.4.2",
    "angular-ui-router": "1.0.0-beta.3",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "bluebird": "^3.4.6",
    "css-loader": "^0.25.0",
    "easy-equals": "1.0.3",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "json-loader": "0.5.4",
    "lodash": "^4.16.3",
    "node-sass": "^3.10.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "tingodb": "file:src\\tingodb-0.5.0.tgz",
    "tungus": "file:src\\tungus-0.0.5.tgz",
    "url-loader": "^0.5.7"
  },
  "devDependencies": {
    "ajv": "^4.7.7",
    "angular-mocks": "^1.5.8",
    "asyncawait": "^1.0.6",
    "chai": "^3.5.0",
    "co": "^4.6.0",
    "copy-dir": "^0.3.0",
    "copy-webpack-plugin": "^3.0.1",
    "electron": "^1.4.2",
    "electron-builder": "^5.23.2",
    "eslint": "3.7.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "jsonix": "^2.4.1",
    "jsonix-schema-compiler": "^2.3.9",
    "mocha": "^2.5.3",
    "mongoose": "^4.6.6",
    "node-uuid": "^1.4.7",
    "number-format.js": "^1.1.10",
    "protractor": "^4.0.0",
    "webpack": "^1.13.1",
    "webpack-merge": "^0.14.1",
    "webpack-validator": "^2.2.7",
    "xml2js": "^0.4.17"
  },
  "build": {
    "appId": "ca.canada.eDossierBuilder",
    "app-category-type": "electron.dossier.builder.type",
    "asar": false,
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "icon": "resources/windows/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "installerHeaderIcon": "resources/windows/setup-icon.ico",
      "installerHeader": "resources/windows/pmralogo.bmp"
    },
    "mac": {
      "icon": "resources/osx/icon.icns"
    },
    "dmg": {
      "icon": "resources/osx/dmg-icon.icns",
      "background": "resources/osx/dmg-background.png"
    }
  },
  "directories": {
    "buildResources": "resources",
    "app": "dist",
    "output": "prepublish"
  }
}
