{
  "name": "edb",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "postinstall": "npm run pack",
    "start": "electron build",
    "clean": "gulp clean",
    "pack": "concurrently \"gulp pack\" \"webpack --watch\"",
    "init": "gulp prebuild && webpack",
    "test": "mocha \"./src/components/**/*.spec.js\" --recursive --compilers js:babel-register -r jsdom/register",
    "tdd": "mocha \"./src/components/**/*.spec.js\" --recursive --compilers js:babel-register --watch",
    "lint": "eslint .",
    "build": "build",
    "backend": "gulp back-watch"
  },
  "author": "alex gagnon",
  "dependencies": {
    "angular": "^1.5.8",
    "angular-animate": "^1.5.8",
    "angular-aria": "^1.5.8",
    "angular-material": "^1.0.9",
    "angular-mocks": "^1.5.8",
    "angular-ui-router": "^0.3.1",
    "angular-ui-tree": "^2.16.0",
    "babel-core": "^6.11.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.11.6",
    "bluebird": "^3.4.1",
    "bootstrap": "^3.3.7",
    "browser-sync": "^2.13.0",
    "chai": "^3.5.0",
    "concurrently": "^2.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "gulp": "^3.9.1",
    "gulp-sass": "^2.3.2",
    "html-webpack-plugin": "^2.22.0",
    "jquery": "^3.1.0",
    "mocha": "^2.5.3",
    "protractor": "^4.0.0",
    "sass-loader": "^4.0.0",
    "webpack-merge": "^0.14.1",
    "webpack-validator": "^2.2.7"
  },
  "devDependencies": {
    "babel-loader": "^6.2.4",
    "css-loader": "^0.23.1",
    "del": "^2.2.1",
    "electron-builder": "^5.23.2",
    "electron-prebuilt": "^1.4.0",
    "eslint": "^3.3.0",
    "file-loader": "^0.9.0",
    "gulp-rename": "^1.2.2",
    "nedb": "^1.8.0",
    "q": "^1.4.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1"
  },
  "build": {
    "appId": "ca.canada.eDossierBuilder",
    "app-category-type": "electron.dossier.builder.type",
    "asar": false,
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "icon": "resources/windows/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "installerHeaderIcon": "resources/windows/setup-icon.ico",
      "installerHeader": "resources/windows/pmralogo.bmp"
    },
    "mac": {
      "icon": "resources/osx/icon.icns"
    },
    "dmg": {
      "icon": "resources/osx/dmg-icon.icns",
      "background": "resources/osx/dmg-background.png"
    }
  },
  "directories": {
    "buildResources": "resources",
    "app": "build"
  }
}
