    "angular-aria": "^1.5.8",
    "angular-material": "^1.0.9",
    "angular-mocks": "^1.5.8",
    "angular-ui-router": "^0.3.1",
    "angular-ui-tree": "^2.16.0",
    "babel-core": "^6.11.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.11.6",
    "bluebird": "^3.4.1",
