{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            1,
            4
        ],
        "semi": [
            1,
            "always"
        ],
        "no-console": 1,
        "no-unused-vars": 1,
        "no-debugger": 1
    }
}